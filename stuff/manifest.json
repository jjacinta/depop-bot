{
  "name": "Depop Bot",
  "version": "1.0",
  "description": "An autmated bot for selling on Depop.",
  "permissions": ["activeTab", "declarativeContent", "storage"],
  "background": {
      "scripts": ["background.js"],
      "persistent": true
  },
  "page_action": {
      "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "manifest_version": 2
}
